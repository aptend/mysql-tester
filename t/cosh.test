drop table if exists t1;
create table t1(a int,b float);
insert into t1 values(0,0),(-15,-20),(-22,-12.5);
insert into t1 values(0,360),(30,390),(90,450),(180,270),(180,180);
select cosh(a*pi()/180) as cosha,cosh(b*pi()/180) coshb from t1;
select cosh(a*pi()/180)*cosh(b*pi()/180) as coshab,cosh(cosh(a*pi()/180)) as c from t1;
select b from t1 where cosh(a*pi()/180)<=cosh(b*pi()/180)  order by a;
